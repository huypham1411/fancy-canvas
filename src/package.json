{
  "name": "fancy-canvas",
  "version": "0.0.0",
  "description": "Functionality on top of HTML canvas element, including support for HiDPI and pixel-perfect rendering",
  "scripts": {
    "build": "npm run build:esmodule && node ../tools/build-package-json package.json ../dist/fancy-canvas/package.json",
    "build:esmodule": "ttsc --project tsconfig.json && node ../tools/replace-extension ../dist/fancy-canvas .js .mjs && node ../tools/replace-extension ../dist/fancy-canvas .ts .mts"
  },
  "keywords": [
    "html",
    "canvas",
    "graphics",
    "hidpi",
    "pixel-perfect"
  ],
  "author": "smakarov@tradingview.com",
  "license": "MIT",
  "private": true,
  "module": "./index.mjs",
  "exports": {
    "import": "./index.mjs"
  }
}
